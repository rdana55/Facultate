<Window x:Class="AgentiiZboruriC.controller.LoginController"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="400" Width="600">
    <Grid>
        <TextBox x:Name="UnTextField" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120" Height="25" Margin="230,150,0,0" TextAlignment="Center" />
        <Button x:Name="Login" Content="Login" Width="75" Height="25" Margin="282,233,0,0" Click="HandleLogin" />
        <Label Content="LOGIN" FontSize="34" HorizontalAlignment="Center" VerticalAlignment="Center" Width="107" Height="50" Margin="251,80,0,0" />
        <PasswordBox x:Name="PsTextField" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120" Height="25" Margin="230,188,0,0" />
    </Grid>
</Window>