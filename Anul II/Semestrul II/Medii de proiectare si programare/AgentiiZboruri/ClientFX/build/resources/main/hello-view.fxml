<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.clientfx.LoginController">
    <children>
        <Pane prefHeight="400.0" prefWidth="600.0">
            <children>
                <TextField fx:id="unTextField" alignment="CENTER" layoutX="230.0" layoutY="150.0" promptText="username" />
                <Button fx:id="login" layoutX="282.0" layoutY="233.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
                <Label layoutX="251.0" layoutY="80.0" prefHeight="50.0" prefWidth="107.0" text="LOGIN">
                    <font>
                        <Font size="34.0" />
                    </font>
                </Label>
            <PasswordField fx:id="psTextField" layoutX="230.0" layoutY="188.0" promptText="password" />
            </children>
        </Pane>
    </children>
</AnchorPane>
